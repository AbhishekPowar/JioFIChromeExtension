{
    "manifest_version":2,
    "name": "Jio Launcher",
    "description": "quick",
    "version": "1.0.0",
    "icons": {"256": "icon.png"},
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab","http://jiofi.local.html/*"],
    "background": {
        "scripts": [
        "background.js"
        ],  
        "persistent": true
      }
  
}